	"s": [
		{
                "minpost": {
                        "endpoint":		"libwebsockets.org",
                        "port":			443,
                        "protocol":		"h1",
                        "http_method":		"POST",
                        "http_url":		"testserver/formtest",
                        "metadata": [
                                { "ctype":		"Content-Type:" }
                        ],
                        "tls":			true,
                        "opportunistic":	true,
                        "allow_redirects":	true,
                        "retry":		"default",
                        "tls_trust_store":	"le_via_isrg"
			}
		}	
	]
