{
	"release":		"1",
	"product":		"sx-http-post",
	"schema-version":	1,
	"retry": [{
		"default": {
			"backoff":	[1000, 2000, 3000, 5000, 10000],
			"conceal":	5,
			"jitterpc":	20,
			"svalidping":	30,
			"svalidhup":	35
		}
	}],
	"certs": [{
	"isrg_root_x1": ""
	}
	],
	"trust_stores": [{
